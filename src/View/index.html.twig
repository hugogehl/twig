{% extends "base.html.twig" %}


{% block title %}
    <h1 style="margin: 50px;">Products</h1>
{% endblock %}

{% block container %}
    <div class="container" style="max-width: 90%; display: flex; flex-wrap: wrap; margin-top: 100px;">
    {% for product in products %}
            <div class="card" style="min-width: 30%;">
                <div class="card-body">
                    <h5 class="card-title">{{ product }}</h5>
                </div>
            </div>
    {% endfor %}
    </div>
{% endblock %}